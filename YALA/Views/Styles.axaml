<Styles xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

	<Styles.Resources>
		<ResourceDictionary>
			<ResourceDictionary.ThemeDictionaries>
				<ResourceDictionary x:Key="Light">
					<SolidColorBrush x:Key="BackgroundBrush" Color="White"/>
					<SolidColorBrush x:Key="ForegroundBrush" Color="Black"/>
					<SolidColorBrush x:Key="PanelBackgroundBrush" Color="#F0F0F0"/>
					<SolidColorBrush x:Key="AccentBrush" Color="#007ACC"/>
				</ResourceDictionary>
				<ResourceDictionary x:Key="Dark">
					<SolidColorBrush x:Key="BackgroundBrush" Color="#1E1E1E"/>
					<SolidColorBrush x:Key="ForegroundBrush" Color="White"/>
					<SolidColorBrush x:Key="PanelBackgroundBrush" Color="#2D2D30"/>
					<SolidColorBrush x:Key="AccentBrush" Color="#0099FF"/>
				</ResourceDictionary>
			</ResourceDictionary.ThemeDictionaries>
		</ResourceDictionary>
	</Styles.Resources>

</Styles>
